<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd" >
<!-- s1017/emp.xml -->
<sqlMap namespace="ajax">
	<select id="select" parameterClass="String" resultClass="String">
		select ename
		from emp
		where upper(ename) like #ename#
	</select>
</sqlMap>
